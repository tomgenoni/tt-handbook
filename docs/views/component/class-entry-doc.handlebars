<h2>Classes</h2>
{{#each this}}
    <div class="entry flex bt b-gray-300">
        <code class="w-2/5 pv2 s:ph3 clipboard">{{className}}</code>
        <code class="w-3/5 pv2 s:ph3" style="opacity: .7">
            {{#each declarations}}
                <div>
                    {{property}}</span>: {{value}}; {{{hex}}}
                </div>
            {{/each}}
        </code>
    </div>
{{/each}}
